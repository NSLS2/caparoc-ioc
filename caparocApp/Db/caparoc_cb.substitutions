 { $(Sys)	, "$(Dev)Cmd:CtrlCh7-Cmd"	, $(ASYNPORT)_CTRL_CHAN_CMD	, 6	, "Off"	, "On"	, "NO_ALARM"	, "NO_ALARM"	, "Channel 7 Pwr Ctrl"	}
 { $(Sys)	, "$(Dev)Cmd:CtrlCh8-Cmd"	, $(ASYNPORT)_CTRL_CHAN_CMD	, 7	, "Off"	, "On"	, "NO_ALARM"	, "NO_ALARM"	, "Channel 8 Pwr Ctrl"	}
 { $(Sys)	, "$(Dev)Cmd:CtrlCh9-Cmd"	, $(ASYNPORT)_CTRL_CHAN_CMD	, 8	, "Off"	, "On"	, "NO_ALARM"	, "NO_ALARM"	, "Channel 9 Pwr Ctrl"	}
 { $(Sys)	, "$(Dev)Cmd:CtrlCh10-Cmd"	, $(ASYNPORT)_CTRL_CHAN_CMD	, 9	, "Off"	, "On"	, "NO_ALARM"	, "NO_ALARM"	, "Channel 10 Pwr Ctrl"	}
