 { $(Sys)	, "$(Dev)Sts:PwrFunc-Sts"	, $(ASYNPORT)_PS_FUNC	, 0	, "0xFFFF"	, "I/O Intr"	, "Status Function Module PS"	}
}

# Channel Error Reset, modbus start address 272
file "bo_word.template" { pattern
 { P		, R				, PORT			, OFFSET, ZNAM	, ONAM		, DESC				}
 { $(Sys)	, "$(Dev)Rst:ErrMod1-Cmd"	, $(ASYNPORT)_ERR_RST	, 0	, "Low"	, "High"	, "Error Reset Module 1"	}
 { $(Sys)	, "$(Dev)Rst:ErrMod2-Cmd"	, $(ASYNPORT)_ERR_RST	, 1	, "Low"	, "High"	, "Error Reset Module 2"	}
 { $(Sys)	, "$(Dev)Rst:ErrMod3-Cmd"	, $(ASYNPORT)_ERR_RST	, 2	, "Low"	, "High"	, "Error Reset Module 3"	}
 { $(Sys)	, "$(Dev)Rst:ErrMod4-Cmd"	, $(ASYNPORT)_ERR_RST	, 3	, "Low"	, "High"	, "Error Reset Module 4"	}
}

