# Sum of nominal currents, hours since reboot, internal temperature, modbus start address 24581
file "longin.template" { pattern
 { P		, R			, PORT			, OFFSET, EGU	, SCAN		, DESC			}
 { $(Sys)	, "$(Dev)I:NomSum-I"	, $(ASYNPORT)_SYS_INP2	, 0	, "A"	, "I/O Intr"	, "Sum of Nominal Currents"	}
# { $(Sys)	, "$(Dev)Time:Boot-I"	, $(ASYNPORT)_SYS_INP2	, 3	, "h"	, "I/O Intr"	, "Hours Since Last Boot"	}
 { $(Sys)	, "$(Dev)T:In-I"	, $(ASYNPORT)_SYS_INP2	, 4	, "C"	, "I/O Intr"	, "Internal Temperature"	}
}

# Input Voltage, Current, modbus start address 24577
file "ai.template" { pattern
 { P		, R			, PORT			, OFFSET, BITS	, ESLO	, EGUL	, EGUF	, PREC	,EGU	, SCAN		, DESC				}
 { $(Sys)	, "$(Dev)I:Total-I"	, $(ASYNPORT)_SYS_INP1	, 0	, 16	, 0.01	, 0.	, 10.	, 3	, "A"	, "I/O Intr"	, "Total System Current"	}
 { $(Sys)	, "$(Dev)E:Total-I"	, $(ASYNPORT)_SYS_INP1	, 1	, 16	, 0.01	, 0.	, 10.	, 3	, "V"	, "I/O Intr"	, "Input Voltage"		}
}

